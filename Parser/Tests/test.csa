:__script_entry__
Function 0 2 0
//Auto assigning 10 statics
push_0
setStatic1 1 //MainMenu_PLAYER_MENU
push_1
setStatic1 2 //MainMenu_VEHICLE_MENU
push_2
setStatic1 3 //MainMenu_WEAPON_MENU
push_3
setStatic1 4 //MainMenu_WORLD_MENU
push_0
setStatic1 5 //PlayerMenu_GODMODE
push_1
setStatic1 6 //PlayerMenu_INFINITE_AMMO
push_0
setStatic1 7 //currentMenu
push_0
setStatic1 8 //currentOption
push_0
setStatic1 9 //open
setStatic1 10 //currentMenuHeight
Call @main
Return 0 0

:draw_text
Function 5 7 0
getF1 1 //font
CallNative SET_TEXT_FONT 1 0
getF1 4 //size
getF1 4 //size
CallNative SET_TEXT_SCALE 2 0
push1 255
push1 255
push1 255
push1 255
CallNative SET_TEXT_COLOUR 4 0
CallNative SET_TEXT_OUTLINE 0 0
PushString "STRING"
CallNative BEGIN_TEXT_COMMAND_DISPLAY_TEXT 1 0
getF1 0 //text
CallNative ADD_TEXT_COMPONENT_SUBSTRING_PLAYER_NAME 1 0
getF1 2 //x
getF1 3 //y
CallNative END_TEXT_COMMAND_DISPLAY_TEXT 2 0
Return 5 0

:show_notification
Function 1 3 0
PushString "STRING"
CallNative BEGIN_TEXT_COMMAND_PRINT 1 0
getF1 0 //message
CallNative ADD_TEXT_COMPONENT_SUBSTRING_PLAYER_NAME 1 0
pushS 2000
push_1
CallNative END_TEXT_COMMAND_PRINT 2 0
Return 1 0

:draw_menu_title
Function 1 3 0
getF1 0 //title
push_1
fpush 0.839844
fpush 0.191832
fpush 1.05
Call @draw_text
Return 1 0

:draw_background
Function 0 2 0
Return 0 0

:draw_menu
Function 0 2 0
PushString "Goy Menu"
Call @draw_menu_title
getStatic1 7 //currentMenu
getStatic1 1 //MainMenu_PLAYER_MENU
JumpNE @selection_end_0
getStatic1 8 //currentOption
Switch [0=@selection_1_case_0][1=@selection_1_case_1]
:selection_1_case_0
Jump @selection_end_1
:selection_1_case_1
Jump @selection_end_1
:selection_end_1
:selection_end_0
Return 0 0

:main
Function 0 2 0
:loop_0
getStatic1 9 //open
Not
JumpFalse @selection_else_2
push_1
setF1 9 //open
:selection_else_2
Call @draw_menu
push_0
setF1 9 //open
:selection_end_2
push_0
CallNative wait 1 0
Jump @loop_0
Return 0 0

